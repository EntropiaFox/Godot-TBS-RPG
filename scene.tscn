script/source = "extends Sprite

var tileW = 8
var tileH = 4

var goalPos

signal has_moved
signal has_clicked

func _ready():
	#get_tree().get_root().add_child(Sprite.new())
	
	goalPos = get_pos()
	set_process_input(true)
	#set_process(true)
	pass

func _input(ev):
	if ev.is_action_pressed(\"ui_left\"):
		translate(Vector2(-tileW, -tileH))
		
		#goalPos += Vector2(-tileW, -tileH)
		emit_signal(\"has_moved\", self)
		
	elif ev.is_action_pressed(\"ui_right\"):
		translate(Vector2(tileW, tileH))
		#goalPos += Vector2(tileW, tileH)
		emit_signal(\"has_moved\", self)
		
	elif ev.is_action_pressed(\"ui_down\"):
		translate(Vector2(-tileW, tileH))
		#goalPos += Vector2(-tileW, tileH)
		emit_signal(\"has_moved\", self)
		
	elif ev.is_action_pressed(\"ui_up\"):
		translate(Vector2(tileW, -tileH))
		#goalPos += Vector2(tileW, -tileH)
		emit_signal(\"has_moved\", self)
	
	elif ev.is_action_pressed(\"ui_accept\"):
		emit_signal(\"has_clicked\", self)

func _process(delta):
	var motion = goalPos - get_pos() 
	translate(motion / 5)

func aaaa(pos):
	var _x = (pos.x / tileW + pos.y / tileH) /2;
	var _y = (pos.y / tileH - pos.x / tileW) /2;
	return Vector2(floor(_x), floor(_y))

func world_to_grid(pos):
	var _x = (pos.x / tileW + pos.y / tileH) / 2;
	var _y = (pos.y / tileH - pos.x / tileW) / 2;
	return Vector2(floor(_x), floor(_y))"

